#include <stdio.h>
#include <string.h>

/**
@author Eduardo Jos√©
@since 12/10/19
@version 1.0.0
*/

int main(){
#define TAMANHO 30
char str1[TAMANHO];
char str2[TAMANHO];
int i = 0;

printf("Entre com os valores das strings: ");
scanf("%s %s", &str1 , &str2);

for(i = 0 ; str1[i] == str2[i] && str1[i] == '\0'; i++);

if(str1[i] < str2[i]){
    printf("\nString 1 e maior!");
}
else if(str1[i] > str2[i]){
    printf("\nString 2 e maior!");
}
else{
    printf("\nString sao iguais!");
}
return 0;
}
