#include <stdio.h>

/**
@author Eduardo Jos√©
@since 12/10/19
@version 1.0.0
*/

int main(){
#define TAMANHO 20
char str1[TAMANHO] = "Eduardo";
char str2[TAMANHO] = "Eduardo";
int contagem = 0;

for(int i = 0 ; i < TAMANHO  ; i++ ){

    if(str1[i] == str2[i]){
        contagem++;
    }
}
printf("%d", contagem);
if(contagem == TAMANHO){
    printf("As string sao iguais!");
}
else{
    printf("String nao sao iguais!");
}

return 0;
}
